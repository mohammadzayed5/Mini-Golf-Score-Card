{
  "appId": "com.MohammadZayed.minigolf",
  "appName": "Mini Golf Score Tracker",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "ios": {
    "contentInset": "automatic",
    "backgroundColor": "#0f172a",
    "allowsLinkPreview": false,
    "scrollEnabled": true,
    "preferences": {
      "ScrollEnabled": true,
      "Orientation": "portrait",
      "KeyboardDisplayRequiresUserAction": false,
      "SuppressesIncrementalRendering": false,
      "GapBetweenPages": 0,
      "PageLength": 0,
      "PaginationBreakingMode": 0,
      "PaginationMode": 0
    }
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#0f172a",
      "showSpinner": true,
      "spinnerColor": "#7cf7c1"
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#0f172a"
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": true
    },
    "Haptics": {},
    "Share": {},
    "Preferences": {
      "group": "minigolf_preferences"
    },
    "Dialog": {}
  }
}